#include <stdio.h>


struct Song {
    char title[100];
    char artist[50];
    float duration; 
};

int main() {
   
    struct Song Song_1 = {"Wanna be yours", "Arctic Monkeys", 3};

    float minutes = Song_1.duration ;

   
    printf("Song Title: %s\n", Song_1.title);
    printf("Artist: %s\n", Song_1.artist);
    printf("Duration: %.2f\n\n", minutes);

  struct Song Song_2 = {"Konji pesida venam", "Chitra & Sriram", 5};

   minutes = Song_2.duration;

   
    printf("Song Title: %s\n", Song_2.title);
    printf("Artist: %s\n", Song_2.artist);
   
    printf("Duration: %.2f\n\n", minutes);

    return 0;
}
