# include <stdio.h>
int main()
{
    int a[2][2]={4,5,4,7},i,j;
    int t[2][2]={0};
    
    for (i=0;i<2;i++) {
        for (j=0;j<2;j++) {
            t[j][i]=a[i][j];
        }
    }
    for (i=0;i<2;i++) {
        for (j=0;j<2;j++) {
            printf("%d ",t[i][j]);
        }
        printf("\n");
    }
    return 0;
}
