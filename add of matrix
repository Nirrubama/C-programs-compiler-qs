# include <stdio.h>
int main()
{
    int a[2][2]={2,5,8,9},i,j;
    int b[2][2]={7,8,9,41};
    int c[2][2]={0};
    
     for (i=0;i<2;i++) {
         for (j=0;j<2;j++) {
             c[i][j]=a[i][j] + b[i][j];
         }
     }
    for (i=0;i<2;i++) {
         for (j=0;j<2;j++) {
             printf("%d ",c[i][j]);
         }
         printf("\n");
     }
     return 0;
}
